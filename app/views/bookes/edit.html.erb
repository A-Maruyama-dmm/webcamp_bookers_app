<h4>Editing Book</h4>
<%= form_with model:@book, url:"/bookes/#{@book.id}", local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <p><%= f.submit 'Update Book' %></p>
<% end %>
<%= link_to "show", booke_path(@book) %>
<%= link_to "back", bookes_path %>